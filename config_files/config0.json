{
    "data": {
        "random_seed": 2112,
        "dataset_dir": "data",
        "dataset_name": "goi4_dp_full_Gipuzkoa",
        "forecasting": true,
        "test_ratio": 0.2,
        "val_ratio": 0.2,
        "resolution": 1,
        "pad": 0,
        "ampute_params": {
            "a": 0.85,
            "b": 10.0
        },
        "subsample_rate": {
            "user": 1,
            "day": 1
        },
        "scaling": {
            "shift": 1,
            "zero_id": -3,
            "log_space": true
        },
        "condition_tag_list": [
            "months",
            "weekdays",
            "users",
            "day_befores"
        ],
        "user_embedding_kwargs": {
            "model_kwargs": {
                "num_topics": 100,
                "num_clusters": 1000,
                "reduce_dim": false,
                "num_lower_dims": null
            },
            "fit_kwargs": {
                "lda": {
                    "perp_tol": 0.5,
                    "max_iter": 100,
                    "batch_size": 64,
                    "verbose": true,
                    "learning_method": "online",
                    "evaluate_every": 5,
                    "n_jobs": null,
                    "doc_topic_prior": 0.05,
                    "topic_word_prior": 0.001
                }
            }
        }
    },
    "model": {
        "latent_dim": 24,
        "distribution_dict": {
            "posterior": {
                "dist_type": "normal",
                "learn_sigma": true,
                "sigma_lim": 0.05,
                "total_max_std": 3.0,
                "mu_upper_lim": 5.0,
                "mu_lower_lim": -5.0,
                "num_neurons": 1000,
                "num_hidden_layers": 2,
                "dropout": false,
                "dropout_rate": 0.2,
                "batch_normalization": true,
                "resnet": false
            },
            "likelihood": {
                "dist_type": "dict-gauss",
                "vocab_size": 100,
                "learn_sigma": true,
                "sigma_lim": 1e-4,
                "marginal_std_lim": 0.1,
                "total_max_std": 5.0,
                "mu_upper_lim": 5.0,
                "mu_lower_lim": -3,
                "num_neurons": 1000,
                "num_hidden_layers": 2,
                "dropout": false,
                "dropout_rate": 0.2,
                "batch_normalization": true,
                "resnet": false
            }
        }
    },
    "train": {
        "lr": 0.001,
        "beta": 1.0,
        "num_mc_samples": 16,
        "validation_mc_samples": 16,
        "weight_decay": 1e-05,
        "gradient_clipping": true,
        "gradient_clipping_kwargs": {
            "max_norm": 1.0
        },
        "lr_scheduling": true,
        "lr_scheduling_kwargs": {
            "threshold": 0.2,
            "factor": 0.5,
            "patience": 3,
            "min_lr": 5e-05
        },
        "earlystopping": true,
        "earlystopping_kwargs": {
            "patience": 10,
            "delta": 0.2
        },
        "epochs": 100,
        "verbose_freq": 250,
        "tensorboard": false,
        "batch_size": 1024,
        "validation_freq": 1000,
        "device": "cuda:1",
        "save_epoch_freq": 5
    },
    "save_dir": "runs/forecast",
    "save_tag": "tmp_"
}